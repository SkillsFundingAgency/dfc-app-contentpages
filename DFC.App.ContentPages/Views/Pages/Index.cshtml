@model IndexViewModel

<!DOCTYPE html>
<html lang="en">
<body>
    @if (Model.Documents?.Count() > 0)
    {
        <ul>
            @foreach (var document in Model.Documents)
            {
                <li>
                    <a href="/pages/@document.Category/@document.CanonicalName">@document.Category - @document.CanonicalName</a>
                </li>
            }
        </ul>
    }
</body>
</html>